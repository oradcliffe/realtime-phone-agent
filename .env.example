# Azure Communication Services configuration
# Format: endpoint=https://your-acs-resource.communication.azure.com/;accesskey=your-access-key
ACS_CONNECTION_STRING=your_acs_connection_string

# Azure OpenAI configuration
AZURE_OPENAI_ENDPOINT=https://your-openai-resource.openai.azure.com/
AZURE_OPENAI_REALTIME_DEPLOYMENT_NAME=gpt-4o-realtime-preview
AZURE_OPENAI_API_VERSION=2024-10-01-preview
AZURE_OPENAI_API_KEY=your_api_key

# Azure AI Search configuration
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_KEY=your_search_admin_key
AZURE_SEARCH_INDEX=products

# Local development only - not required for App Service deployment
# Set this to your dev tunnel URL when developing locally
CALLBACK_URI_HOST=https://your-devtunnel-url.use.devtunnels.ms:8080